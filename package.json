{
  "name": "@chancellor-games/exile",
  "version": "0.0.1",
  "description": "Redux helper library for creating a Chancellor Games API client",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "Christopher Giroir <kelsin@valefor.com>",
  "license": "MIT",
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "body-max-line-length": [
        0,
        "always"
      ],
      "footer-max-line-length": [
        0,
        "always"
      ],
      "header-max-length": [
        0,
        "always"
      ]
    }
  },
  "prettier": {
    "plugins": [
      "@ianvs/prettier-plugin-sort-imports"
    ],
    "importOrder": [
      "<BUILTIN_MODULES>",
      "",
      "<THIRD_PARTY_MODULES>",
      "",
      "^ramda$",
      "",
      "^@(/.*)$",
      "^#(.*)$",
      "^[.]",
      "",
      "^@tests(/.*)$",
      "",
      "^(?!.*[.]css$)[./].*$",
      ".css$"
    ],
    "importOrderCaseSensitive": true
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@eslint/js": "^9.18.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.1",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^15.14.0",
    "lefthook": "^1.10.7",
    "prettier": "^3.4.2"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.5.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-redux": "^9.2.0"
  }
}
